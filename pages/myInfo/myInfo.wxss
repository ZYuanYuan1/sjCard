.userInfo-cell {
    background-color: white;
    height: 45px;
    display:flex;
    align-items: center;
    flex-direction: row;
    margin-top: 1px;
}
.userInfo-cell-img {
    height: 65px;
}
.userInfo-cell-label {
    font-size: 14px;
    text-align: left;
    flex-grow:1;
    margin-left: 12px;
}
.userInfo-cell-sublabel {
    font-size: 14px;
    text-align: left;
    padding-right: 24px;
    color:#adadad;
}
.userInfo-cell-icon {
    height: 29px;
    width: 29px;
    margin-left: 12px;
}

.userInfo-cell-arrow {
    position:relative;
    width: 8px;
    height: 13px;
    right: 12px;
}
.contact-button {
    padding-left: 12px;
}

.contact-button-text {
    font-size: 14px;
    flex-grow:1;
    padding-left: 0px;
}
.peopleimg{
  flex:0 0 120rpx;
  height:120rpx;
  margin-right: 60rpx;
}
.peopleimg image{
  width:100%;
  height:100%;
  border-radius:50%;
}
