.modifyoutter{
  padding:20rpx 24rpx;
  font-size:28rpx;
  display:flex;
justify-content:space-between;

}
view.surebtn{
  flex:0 0 70rpx;
  text-align:right;
}
.nameVal{
  flex:1;
  border:2rpx solid #ddd;
  border-radius:30rpx;
  padding-left:10rpx;
  box-sizing:border-box;
  vertical-align:middle;
  height:80rpx;
}

.singlelinebox{
  display:flex;
  justify-content: space-between;
  padding-left:20rpx;
  background-color:#fff;
  border-bottom:2rpx solid #e8e8ed;
  align-items: center;
  font-size: 28rpx;
  color:#333;
}

.nextpageicon image{
  width:14rpx;
  height:24rpx;
  margin-right:16rpx;
}