/* pages/sureOrders/sureOrders.wxss */
@import "../../components/six-input-box/component.wxss";
page{
  /* min-height:100%; */
  background-color:#f6f6f6;
}
.orderinfobox{
  display:flex;
  padding:30rpx;
  background-color:#fff;
  margin:20rpx 0;
}
.orderimgbox{
  flex:0 0 120rpx;
  margin-right:20rpx;
  height:120rpx;
}
.orderimgbox image{
  width:100%;
  height:100%;
}
view.introtextbox{
  flex:1;
  font-size:28rpx;
  color:#cbcbcb;

}
.orderheadline{
  font-size: 32rpx;
  color:#333;
  overflow:hidden;
text-overflow:ellipsis;
display:-webkit-box;
-webkit-line-clamp:1;
-webkit-box-orient:vertical;
margin-bottom: 30rpx;
}
.computemoney{
  background-color:#fff;
}
.moneylineone{
  border-bottom:2rpx solid #ddd;
}
.moneylineone,.moneylinetwo{
  padding:0 30rpx;
  height:90rpx;
  line-height:90rpx;
  box-sizing:border-box;
  font-size:26rpx;
  color:#000;
}
.moneylineone text,.moneylinetwo text{
  color:#f3362b;
  font-size:32rpx;
}
/*底部按钮*/
.btnbox{
  position:fixed;
  bottom:2%;
  width:100%;

}
.btnbox button{
  width:90%;
background-color:#f5bf00;
color:#fff;
font-size:38rpx;
letter-spacing:4rpx;
}
.page:after{
  content:"";
  display:block;
  clear:both;
  height:150rpx;
}
/*弹框样式*/
.titleright{
  color:#030303;
  font-size:36rpx;
}
.modal-content{
 border-top:2rpx solid #91b49c;
  text-align:center;
  padding:30rpx 30rpx 38rpx 30rpx;
}
.hosthead{
  color:#010000;
  font-size:32rpx;
  margin-bottom: 36rpx;

}
.moneytext{
  font-size: 40rpx;
  color:#000;
  font-weight:700;
  margin-bottom:46rpx;
}
view.bankbox{
  display:flex;
  padding:26rpx 0;
  border-top:2rpx solid #d8d8d8;
  border-bottom:2rpx solid #d8d8d8;
  justify-content: space-around;
  margin-bottom: 40rpx;
}
.bankminicon{
  width:55rpx;
  height:55rpx;
  border-radius:50%;
  vertical-align:middle;

}
.rightminicon{
  width:22rpx;
  height:28rpx;
  position:relative;
top:10rpx;
vertical-align:middle;

}
.modal-dialog {
  top:6%;
  margin:0rpx 105rpx;
}

.maintext{
  padding:0rpx 30rpx;
  color:#333;
  line-height:50rpx;
}
.maintext view{
  font-size:28rpx;
  text-indent: 56rpx;
}
.maintext text{
  font-size:32rpx;
}

.namebox{
 margin:0 36rpx;
font-size:28rpx;
color:#333;
line-height:80rpx;
border-bottom:2rpx solid #ddd;

}
.namebox input{
  display:inline-block;
vertical-align:middle;
width:66%;
padding:0;

}
.moneybox{
  margin:20px 40rpx 30rpx 40rpx;
}
.star{
  color:#e80000;
}
.remark{
  margin-top: 30rpx;
}