/* pages/creditScore/creditScore.wxss */
@import "../../components/six-input-box/component.wxss";
.imgbox{
  text-align:center;
  margin:46rpx 0;
}
.imgbox image{
  width:530rpx;
  height:330rpx;
}
view.scorehead{
  text-align:center;
  font-size:40rpx;
  color:#333;
  margin-bottom:48rpx;
}
.creditintrotext{
  color:#666;
  font-size:28rpx;
  padding:0rpx 55rpx;
  line-height:50rpx;
}
/*底部按钮*/
.btnbox{
  position:fixed;
  bottom:2%;
  width:100%;
}
.btnbox button{
  width:90%;
background-color:#999;
color:#fff;
font-size:38rpx;
letter-spacing:4rpx;
}
.page:after{
  content:"";
  display:block;
  clear:both;
  height:150rpx;
}
/*弹框样式*/
.titleright{
  color:#030303;
  font-size:36rpx;
}
.modal-content{
 border-top:2rpx solid #91b49c;
  text-align:center;
  padding:30rpx 30rpx 38rpx 30rpx;
}
.hosthead{
  color:#010000;
  font-size:32rpx;
  margin-bottom: 36rpx;

}
.moneytext{
  font-size: 40rpx;
  color:#000;
  font-weight:700;
  margin-bottom:46rpx;
}
view.bankbox{
  display:flex;
  padding:26rpx 0;
  border-top:2rpx solid #d8d8d8;
  border-bottom:2rpx solid #d8d8d8;
  justify-content: space-around;
  margin-bottom: 40rpx;
}
.bankminicon{
  width:55rpx;
  height:55rpx;
  border-radius:50%;
  vertical-align:middle;

}
.rightminicon{
  width:22rpx;
  height:28rpx;
  position:relative;
top:10rpx;
vertical-align:middle;

}
.modal-dialog {
  top:6%;
  margin:0rpx 105rpx;
}
view.canbuybtnbox button{
  background-color:#f5bf00;
}